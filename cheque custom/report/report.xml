<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>



        <!-- Cheque Report   -->
        <record id="paperformat_cheque" model="report.paperformat">
            <field name="name">Check</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="page_height">89</field>
            <field name="page_width">210</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">4</field>
            <field name="margin_bottom">0</field>
            <field name="margin_left">30</field>
            <field name="margin_right">0</field>
            <field name="header_line" eval="False" />
            <field name="header_spacing">0</field>
            <field name="dpi">80</field>
        </record>
        <report
                string="Cheque"
                id="report_cheque"
                model="account.payment"
                report_type="qweb-pdf"
                name="custom_check_print.report_cheque_qweb"
                file="Cheque"
                paperformat="custom_check_print.paperformat_cheque"
                menu="True"
        />

    </data>

</odoo>